{
  "name": "books-management",
  "version": "1.0.0",
  "description": "this tutorial is all about books management",
  "main": "index.js",
  "config": {
    "gulp": "./node_modules/.bin/gulp"
  },
  "scripts": {
    "authors-js": "gulp --gulpfile pages/authors/gulpfile.js authors-dev-js --env=dev",
    "authors-css": "gulp --gulpfile pages/authors/gulpfile.js authors-dev-css --env=dev",
    "authors-js-css-dev": "concurrently  \"npm run authors-js\" \"npm run authors-css\" ",
    "authors-production": "gulp --gulpfile pages/authors/gulpfile.js authors-production --env=dist",

    "books-js": "gulp --gulpfile pages/books/gulpfile.js books-dev-js --env=dev",
    "books-css": "gulp --gulpfile pages/books/gulpfile.js books-dev-css --env=dev",
    "books-js-css-dev": "concurrently  \"npm run books-js\" \"npm run books-css\" ",
    "books-production": "gulp --gulpfile pages/books/gulpfile.js books-production --env=dist"
  },
  "keywords": [
    "vanilla",
    "webpack",
    "npm",
    "node",
    "gulp",
    "es6"
  ],
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-postcss": "^6.1.1",
    "gulp-sass": "^2.3.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "lodash": "^4.11.2",
    "minimist": "^1.2.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-stream": "^3.1.0"
  },
  "author": "David Nguyen",
  "license": "MIT",
  "dependencies": {}
}
